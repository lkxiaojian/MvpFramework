<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MZBannerView">
        <attr name="open_mz_mode" format="boolean"/>
        <attr name="canLoop" format="boolean"/>
        <attr name="indicatorPaddingLeft" format="dimension"/>
        <attr name="indicatorPaddingRight" format="dimension"/>
        <attr name="indicatorAlign">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="BannerTabHost">
        <attr name="LeftIcon" format="reference"/>
        <attr name="TabText" format="string"/>
        <attr name="CheckState" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="BasicItemView">
        <attr name="Icon" format="reference"/>
        <attr name="Title" format="string"/>
    </declare-styleable>
    <declare-styleable name="AddInfoItem">
        <attr name="leftTitle" format="string" />
    </declare-styleable>
    <declare-styleable name="SettingItemView">
        <attr name="title" format="string" />
        <attr name="desc_on" format="string" />
        <attr name="desc_off" format="string" />
    </declare-styleable>
    <declare-styleable name="PersonInfoItem">
        <attr name="detail_title" format="string"/>
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>




    <declare-styleable name="EasyPullLayout"><attr format="dimension" name="trigger_offset_left"/><attr format="dimension" name="trigger_offset_top"/><attr format="dimension" name="trigger_offset_right"/><attr format="dimension" name="trigger_offset_bottom"/><attr format="dimension" name="max_offset_left"/><attr format="dimension" name="max_offset_top"/><attr format="dimension" name="max_offset_right"/><attr format="dimension" name="max_offset_bottom"/><attr format="boolean" name="fixed_content_left"/><attr format="boolean" name="fixed_content_top"/><attr format="boolean" name="fixed_content_right"/><attr format="boolean" name="fixed_content_bottom"/><attr format="integer" name="roll_back_duration"/><attr format="float" name="sticky_factor"/></declare-styleable>
    <declare-styleable name="EasyPullLayout_LayoutParams"><attr format="enum" name="layout_type">
        <enum name="edge_left" value="0"/>
        <enum name="edge_top" value="1"/>
        <enum name="edge_right" value="2"/>
        <enum name="edge_bottom" value="3"/>
        <enum name="content" value="4"/>
    </attr><attr format="boolean" name="layout_content_fixed"/></declare-styleable>

</resources>